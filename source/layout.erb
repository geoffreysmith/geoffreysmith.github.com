    <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
    <%= stylesheet_link_tag "theme" %>
  </head>
  <body>
    
    <div class="canvas">
      <header class="page-header">
        <div class="header-inner">
          <div class="logo"><%= link_to 'New CMS Quarterly', '/' %></div>
